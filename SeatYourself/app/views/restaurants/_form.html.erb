<%= form_for @restaurant do |f| %>

<%= f.label :name %><br />
<%= f.text_field :name %>

<%= f.label :address %><br />
<%= f.text_field :address %>

<%= f.label :phone_number %><br />
<%= f.text_field :phone_number %>

<%= f.label :website_url %><br />
<%= f.text_field :website_url %>

<%= f.label :image %><br />
<%= f.file_field :image %>

<%= f.submit %>

<%end%>

